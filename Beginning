        System.out.println("Your Last Name is "+lastname);
        System.out.println("Input Home Town: ");
        String hometown=scanner.nextLine();
        System.out.println("Your Home Town is "+hometown);
        System.out.println("Input Date of Birth(ddmmyyyy): ");
        int DOB=scanner.nextInt();
        System.out.println("Input KCSE Grade: ");
        String grade=scanner.nextLine();
        System.out.println("Your KCSE Grade is "+grade);
